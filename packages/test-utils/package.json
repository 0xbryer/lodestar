{
  "name": "@lodestar/test-utils",
  "version": "1.18.0",
  "description": "Test utilities reused across other packages",
  "author": "ChainSafe Systems",
  "scripts": {
    "clean": "rm -rf lib && rm -f *.tsbuildinfo",
    "build:esm": "tsc -p tsconfig.esm.json",
    "prebuild:cjs": "node ../../scripts/fix_dirname_and_filename.mjs pre test-utils",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "postbuild:cjs": "node ../../scripts/fix_dirname_and_filename.mjs post test-utils",
    "build": "yarn build:cjs && yarn build:esm",
    "postbuild": "node ../../scripts/fix_dual_modules_lib.mjs test-utils",
    "build:release": "yarn clean && yarn build",
    "build:watch": "yarn run build --watch",
    "check-build": "node -e \"(async function() { await import('./lib/esm/index.mjs') })()\"",
    "check-types": "tsc",
    "lint": "eslint --color --ext .ts src/",
    "lint:fix": "yarn run lint --fix",
    "check-readme": "typescript-docs-verifier"
  },
  "dependencies": {
    "@chainsafe/bls": "^8.1.0",
    "@chainsafe/bls-keystore": "^3.0.1",
    "@lodestar/params": "^1.18.0",
    "@lodestar/utils": "^1.18.0",
    "axios": "^1.3.4",
    "testcontainers": "^10.2.1",
    "tmp": "^0.2.1",
    "vitest": "^1.2.1"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.24"
  },
  "peerDependencies": {
    "vitest": "^1.2.1"
  },
  "type": "module",
  "main": "./lib/cjs/index.cjs",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.cjs"
    },
    "./doubles": {
      "import": "./lib/esm/doubles.js",
      "require": "./lib/cjs/doubles.cjs",
      "types": "./lib/esm/doubles.d.mts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "lib/esm/*",
        "lib/esm/*/index"
      ]
    }
  },
  "files": [
    "*.map",
    "*.cjs",
    "*.d.cts",
    "*.mjs",
    "*.d.mts",
    "lib/**/*.map",
    "lib/esm/**/*.mjs",
    "lib/esm/**/*.d.mts",
    "lib/cjs/**/*.cjs",
    "lib/cjs/**/*.d.cts"
  ],
  "keywords": [
    "ethereum",
    "eth-consensus",
    "beacon",
    "blockchain"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/ChainSafe/lodestar#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com:ChainSafe/lodestar.git"
  },
  "bugs": {
    "url": "https://github.com/ChainSafe/lodestar/issues"
  }
}