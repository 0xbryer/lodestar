{
  "name": "@lodestar/params",
  "version": "1.18.0",
  "description": "Chain parameters required for lodestar",
  "author": "ChainSafe Systems",
  "scripts": {
    "clean": "rm -rf lib && rm -f *.tsbuildinfo",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "yarn build:esm && yarn build:cjs",
    "postbuild": "node ../../scripts/fix_dual_modules_lib.mjs params",
    "build:release": "yarn clean && yarn build",
    "build:watch": "yarn run build --watch",
    "check-build": "node -e \"(async function() { await import('./lib/esm/index.mjs') })()\"",
    "check-types": "tsc",
    "lint": "eslint --color --ext .ts src/ test/",
    "lint:fix": "yarn run lint --fix",
    "test": "yarn test:unit",
    "test:unit": "vitest --run --dir test/unit/",
    "test:browsers": "yarn test:browsers:chrome && yarn test:browsers:firefox && yarn test:browsers:electron",
    "test:browsers:chrome": "vitest --run --browser chrome --config ./vitest.browser.config.ts --dir test/unit",
    "test:browsers:firefox": "vitest --run --browser firefox --config ./vitest.browser.config.ts --dir test/unit",
    "test:browsers:electron": "echo 'Electron tests will be introduced back in the future as soon vitest supports electron.'",
    "test:e2e": "vitest --run --config vitest.e2e.config.ts --dir test/e2e/",
    "check-readme": "typescript-docs-verifier"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "axios": "^1.3.4",
    "js-yaml": "^4.1.0"
  },
  "type": "module",
  "main": "./lib/cjs/index.cjs",
  "module": "./lib/esm/index.mjs",
  "types": "./lib/esm/index.d.mts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.mjs",
      "require": "./lib/cjs/index.cjs"
    },
    "./presets/mainnet": {
      "import": "./lib/esm/presets/mainnet.mjs",
      "require": "./lib/cjs/presets/mainnet.cjs"
    },
    "./presets/minimal": {
      "import": "./lib/esm/presets/minimal.mjs",
      "require": "./lib/cjs/presets/minimal.cjs"
    },
    "./presets/gnosis": {
      "import": "./lib/esm/presets/gnosis.mjs",
      "require": "./lib/cjs/presets/gnosis.cjs"
    },
    "./setPreset": {
      "import": "./lib/esm/setPreset.mjs",
      "require": "./lib/cjs/setPreset.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "lib/*",
        "lib/*/index"
      ]
    }
  },
  "files": [
    "*.map",
    "*.cjs",
    "*.d.cts",
    "*.mjs",
    "*.d.mts",
    "lib/**/*.map",
    "lib/esm/**/*.mjs",
    "lib/esm/**/*.d.mts",
    "lib/cjs/**/*.cjs",
    "lib/cjs/**/*.d.cts"
  ],
  "keywords": [
    "ethereum",
    "eth-consensus",
    "beacon",
    "blockchain"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/ChainSafe/lodestar#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com:ChainSafe/lodestar.git"
  },
  "bugs": {
    "url": "https://github.com/ChainSafe/lodestar/issues"
  }
}